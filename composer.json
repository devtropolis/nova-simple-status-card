{
    "name": "devtropolis/nova-simple-status-card",
    "description": "A Laravel Nova card.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Devtropolis\\NovaSimpleStatusCard\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devtropolis\\NovaSimpleStatusCard\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
